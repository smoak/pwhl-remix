{
  "name": "pwhl-remix",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "run-s -n 'generate:css -- --minify'",
    "build:remix": "remix build",
    "dev": "run-p -n dev:*",
    "dev:css": "run-s -n 'generate:css -- --watch'",
    "dev:remix": "remix dev --manual",
    "generate:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "generate:icons": "npx @svgr/cli --out-dir app/components/icons -- app/icons",
    "lint": "run-s 'lint:*'",
    "lint:es": "eslint app --max-warnings=0",
    "lint:style": "prettier 'app/**/*.(ts|tsx)' --check",
    "start": "run-s -n dev",
    "test": "TZ=UTC jest --maxWorkers=50%",
    "test:watch": "TZ=UTC jest --watch --maxWorkers=25%",
    "test:ci": "TZ=UTC jest --runInBand",
    "typecheck": "tsc -b"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@vercel/analytics": "^1.0.2",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "firebase": "^10.7.1",
    "isbot": "^3.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.4.1",
    "@remix-run/eslint-config": "^2.0.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^5.1.6"
  },
  "volta": {
    "node": "18.19.0"
  },
  "engines": {
    "node": ">= 18"
  }
}
